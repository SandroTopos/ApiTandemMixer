@model IEnumerable<WebAPITandemMixer.Models.Phase>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CompoundName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phase1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhaseTypeNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CompoundName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Stage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phase1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhaseTypeNavigation.PhaseType1)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PhaseId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PhaseId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PhaseId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
